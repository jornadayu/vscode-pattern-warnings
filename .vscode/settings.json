{
	"editor.insertSpaces": false,
	"editor.formatOnSave": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"javascript.validate.enable": false,
	"[typescript]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true
		}
	},
	"[json]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	}
}
